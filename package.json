    "npm": "please-use-yarn"
  },
  "scripts": {
    "start:dev": "cp ./project/develop.environment.env .env && nodemon --exec ts-node -r tsconfig-paths/register --files src/App.ts -e ts",
    "start:prod": "cp ./project/production.environment.env .env && nodemon --exec ts-node -r tsconfig-paths/register --files build/App.ts -e ts",
    "clean": "rm -rf build",
    "build:compile": "npx babel src --extensions .ts --out-dir build --source-maps --env-name build",
    "build:types": "tsc",
    "build:test": "yarn lint && yarn build:dev",
    "build:dev": "cp ./project/develop.environment.env .env && rm -rf build && yarn build:compile && yarn build:types && cp -rf ./src/Resources build",
    "build:prod": "cp ./project/production.environment.env .env && rm -rf build && yarn build:compile && yarn build:types && cp -rf ./src/Resources build",
    "prettier": "prettier --write \"src/**/*.ts\"",
    "prettier:check": "prettier --check \"src/**/*.ts\"",
    "lint": "eslint --ignore-path .eslintignore \"src/**/*.ts\" --fix",
    "lint:check": "eslint --ignore-path .eslintignore \"src/**/*.ts\"",
    "prepare": "husky install",
    "migration:generate": "typeorm-ts-node-commonjs migration:generate -d src/Database/AppDataSource.ts",
    "migration:create": "cd ./src/Database/Migrations && typeorm migration:create",
    "migration:show": "typeorm-ts-node-commonjs migration:show -d src/Database/AppDataSource.ts",
    "migration:run": "typeorm-ts-node-commonjs migration:run -d src/Database/AppDataSource.ts",
    "migration:revert": "typeorm-ts-node-commonjs migration:revert -d src/Database/AppDataSource.ts",
    "migration:seed": "ts-node -r tsconfig-paths/register --files src/Database/Seeds/Master.ts -e ts"
  },
